{
	"name": "@marianmeres/stuic",
	"version": "2.0.0-next.5",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"package": "npm run prepack",
		"package:watch": "svelte-kit sync && svelte-package --watch && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"test": "vitest --dir src/"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@marianmeres/icons-fns": "^4.3.1",
		"@marianmeres/random-human-readable": "^1.6.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.21.5",
		"@sveltejs/package": "^2.3.11",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/cli": "^4.1.10",
		"@tailwindcss/forms": "^0.5.10",
		"@tailwindcss/typography": "^0.5.16",
		"@tailwindcss/vite": "^4.1.10",
		"dotenv": "^16.5.0",
		"prettier": "^3.5.3",
		"prettier-plugin-svelte": "^3.4.0",
		"publint": "^0.3.12",
		"svelte": "^5.34.3",
		"svelte-check": "^4.2.1",
		"tailwindcss": "^4.1.10",
		"typescript": "^5.8.3",
		"vite": "^6.3.5",
		"vitest": "^3.2.3"
	},
	"packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
	"dependencies": {
		"@marianmeres/clog": "^2.2.3",
		"@marianmeres/item-collection": "^1.2.15",
		"@marianmeres/parse-boolean": "^1.1.7",
		"@marianmeres/ticker": "^1.15.0",
		"clsx": "^2.1.1",
		"esm-env": "^1.2.2",
		"runed": "^0.23.4",
		"tailwind-merge": "^3.3.1"
	}
}
