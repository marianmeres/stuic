{
	"name": "@marianmeres/stuic",
	"version": "2.56.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pnpm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"package": "pnpm run prepack",
		"package:watch": "svelte-kit sync && svelte-package --watch && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "eslint . && prettier --check .",
		"test": "vitest --dir src/",
		"svelte-check": "svelte-check",
		"svelte-package": "svelte-package",
		"rp": "pnpm run build && ./release.sh patch",
		"rpm": "pnpm run build && ./release.sh minor"
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@marianmeres/icons-fns": "^5.0.0",
		"@marianmeres/random-human-readable": "^1.6.1",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/kit": "^2.49.5",
		"@sveltejs/package": "^2.5.7",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/cli": "^4.1.18",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.18",
		"@types/node": "^25.0.9",
		"dotenv": "^16.6.1",
		"eslint": "^9.39.2",
		"globals": "^16.5.0",
		"prettier": "^3.8.0",
		"prettier-plugin-svelte": "^3.4.1",
		"publint": "^0.3.16",
		"svelte": "^5.46.4",
		"svelte-check": "^4.3.5",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.53.0",
		"vite": "^6.4.1",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@marianmeres/clog": "^3.15.0",
		"@marianmeres/item-collection": "^1.3.5",
		"@marianmeres/parse-boolean": "^2.0.5",
		"@marianmeres/ticker": "^1.16.5",
		"esm-env": "^1.2.2",
		"runed": "^0.23.4",
		"tailwind-merge": "^3.4.0"
	}
}
