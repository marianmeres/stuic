{
	"name": "@marianmeres/stuic",
	"version": "1.117.0",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package && node ./scripts/date.js",
		"build:watch": "fswatch -o src | xargs -n1 -I{} npm run build",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"prettier": "npm run format",
		"release:patch": "release -v patch",
		"release": "release -v minor",
		"test": "vitest"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^4.0.0 || ^5.0.0-next.1"
	},
	"devDependencies": {
		"@marianmeres/parse-boolean": "^1.1.7",
		"@marianmeres/random-human-readable": "^1.6.1",
		"@marianmeres/release": "^1.1.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.27.4",
		"@sveltejs/package": "^2.0.0",
		"@tailwindcss/forms": "^0.5.7",
		"@types/node": "^20.11.0",
		"autoprefixer": "^10.4.16",
		"clsx": "^2.0.0",
		"nodemon": "^3.0.2",
		"postcss": "^8.4.32",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"publint": "^0.1.9",
		"sass": "^1.69.5",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2",
		"vitest": "^1.1.3"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@marianmeres/clog": "^1.0.1",
		"@marianmeres/icons-fns": "^4.0.4",
		"@marianmeres/store": "^1.5.0",
		"@marianmeres/switch-store": "^1.3.1",
		"@marianmeres/ticker": "^1.5.0",
		"dset": "^3.1.3",
		"esm-env": "^1.0.0",
		"klona": "^2.0.6",
		"tailwind-merge": "^2.1.0"
	}
}
